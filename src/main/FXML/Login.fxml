<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="0.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
   <children>
      <AnchorPane prefHeight="287.0" prefWidth="310.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Username">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField fx:id="login_username_field" layoutX="14.0" layoutY="44.0" prefHeight="26.0" prefWidth="276.0" />
            <Label layoutX="14.0" layoutY="86.0" text="Password">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <PasswordField fx:id="login_password_field" layoutX="14.0" layoutY="118.0" prefHeight="26.0" prefWidth="276.0" />
            <Button fx:id="login_btn" layoutX="17.0" layoutY="233.0" mnemonicParsing="false" onAction="#handleLoginSubmit" text="Login">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Label fx:id="login_error_label" layoutX="14.0" layoutY="164.0" textFill="#b20000">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
